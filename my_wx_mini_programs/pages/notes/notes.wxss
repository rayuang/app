.search {
  position: relative;
  margin-bottom: 20rpx;
}

.search image {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 10rpx;
  left: 30rpx;
}

input {
  width: 650rpx;
  height: 60rpx;
  background-color: rgb(240, 241, 243);
  border-radius: 10rpx;
  margin: 10rpx 20rpx;
  padding-left: 60rpx;
}

.notes {
  padding: 0 20rpx;
}
.note_list{
  border-bottom: 2rpx solid #aabbcc;
  padding: 30rpx 0;
  color: #666666;
}
.note_title{
  font-weight: bolder;
  margin-bottom: 20rpx;
}
.note_title text{
  font-size: 52rpx;
}
.note_content {
  display: block;
  text-overflow: ellipsis;
  word-wrap: break-word;
}